{
  "manifest_version": 3,
  "name": "Simple Extension",
  "description": "Add the reading time to Chrome Extension documentation articles",
  "version": "1.0",
  "icons": {},
  "background": {
    "service_worker": "js/service-worker.js"
  },
  "content_scripts": [
    {
      "js": ["./js/content.js"],
      "matches": ["https://*.wikipedia.org/*"]
    }
  ]
}
