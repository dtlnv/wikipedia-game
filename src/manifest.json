{
    "manifest_version": 3,
    "name": "Wiki Quiz",
    "description": "Add the reading time to Chrome Extension documentation articles",
    "version": "1.0",
    "icons": {},
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [
        {
            "css": ["./styles.css"],
            "js": ["./content.js"],
            "matches": ["https://*.wikipedia.org/*"]
        }
    ]
}
